{
  "name": "@asd14/tape-ui",
  "version": "0.2.0",
  "description": "Terminal UI for Tape test runner",
  "license": "BSD-3",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/asd14/tape-ui.git"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "bugs": {
    "url": "https://github.com/asd14/tape-ui/issues"
  },
  "homepage": "https://github.com/asd14/tape-ui",
  "main": "dist/index.js",
  "bin": {
    "tape-ui": "bin/cli.js"
  },
  "keywords": [
    "terminal",
    "cli",
    "testing",
    "tape",
    "tap",
    "blessed"
  ],
  "scripts": {
    "----UTILS------------------------------------": "",
    "clean": "rm -rf dist/* && rm -rf ./node_modules && rm -rf ./flow-typed",
    "setup": "npm run clean && npm i && flow-typed install && npm audit fix",
    "update": "npm-check -u",
    "----DEV--------------------------------------": "",
    "start:dev": "DEBUG=TapeUI:* nodemon --require pretty-error/start --watch src --ignore *-test.js src/index.js",
    "start": "npm run build && node bin/cli.js",
    "----BUILD------------------------------------": "",
    "build": "rm -rf dist/* && babel src --out-dir dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm audit fix && npm test && npm run lint",
    "----LINTING----------------------------------": "",
    "lint:flow": "flow status",
    "lint:md": "markdownlint *.md",
    "lint:js": "eslint --ext .jsx,.js src/",
    "lint": "npm run lint:md && npm run lint:js",
    "----TESTS------------------------------------": "",
    "test": "tape src/**/*.test.js",
    "tdd": "nodemon --watch src --exec \"npm test\""
  },
  "dependencies": {
    "@asd14/eslint-config": "4.0.2",
    "@asd14/m": "0.16.1",
    "chalk": "2.4.1",
    "debug": "4.1.1",
    "figures": "2.0.0",
    "glob": "7.1.3",
    "minimist": "1.2.0",
    "neo-blessed": "0.2.0",
    "react": "16.7.0",
    "react-blessed": "0.5.0",
    "tape": "4.9.2"
  },
  "peerDependencies": {
    "tape": "^4.9.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-compat": "2.6.3",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.3.2",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-unicorn": "6.0.1",
    "flow-bin": "0.89.0",
    "flow-typed": "2.5.1",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.9",
    "npm-check": "5.9.0",
    "prettier": "1.15.3",
    "pretty-error": "2.1.1"
  }
}
